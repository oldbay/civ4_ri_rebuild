	<!-- MOD - START - Trait Attitude Modifier -->
	<TEXT>
		<Tag>TXT_KEY_MISC_ATTITUDE_TRAIT</Tag>
		<English>%D1: "We find you really charming" (from traits)</English>
		<German>%D1: "Wir finden Sie sehr charmant" (aus Merkmalen)</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_MISC_ATTITUDE_TRAIT_NEG</Tag>
		<English>%D1: "We find you really hard to deal with" (from traits)</English>
		<German>%D1: "Wir finden es sehr schwer, mit Ihnen umzugehen" (aus Merkmalen)</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_TRAIT_ATTITUDE</Tag>
		<English>[NEWLINE][SPACE][SPACE][ICON_BULLET]%D1 to diplomatic relations</English>
		<German>[NEWLINE][SPACE][SPACE][ICON_BULLET]%D1 zu diplomatischen Beziehungen</German>
	</TEXT>
	<!-- MOD - END - Trait Attitude Modifier -->
		<TEXT>
		<Tag>TXT_KEY_MISC_ATTITUDE_FIRST_IMPRESSION</Tag>
		<English>%D1: "A first impression is a lasting one."</English>
		<German>%D1: "Der erste Eindruck z&#228;hlt."</German>
	</TEXT>
	<!-- advc.sha: Removed the déjà from French translation. -->
	<TEXT>
		<Tag>TXT_KEY_MISC_ATTITUDE_TEAM_SIZE</Tag>
		<English>%D1: "Your team is too big."</English>
		<German>%D1: "Euer Team ist zu groß."</German>
	</TEXT>
	<!-- advc.sha: Text was "Your small civilization is no threat to us".
		 A bit misleading; it's not a matter of size. -->
	<TEXT>
		<Tag>TXT_KEY_MISC_ATTITUDE_BETTER_RANK</Tag>
		<English>%D1: "You're hardly a threat to us."</English>
		<German>%D1: "Ihr seid kaum eine Bedrohung f&#252;r uns."</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_GOOD_ATTITUDE_TOWARDS</Tag>
		<English>%s1_attitude ([COLOR_POSITIVE_TEXT]+%d2[COLOR_REVERT]) towards %s3_leader</English>
		<German>%s1_attitude ([COLOR_POSITIVE_TEXT]+%d2[COLOR_REVERT]) gegen&#252;ber %s3_leader</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_BAD_ATTITUDE_TOWARDS</Tag>
		<English>%s1_attitude ([COLOR_WARNING_TEXT]%d2[COLOR_REVERT]) towards %s3_leader</English>
		<German>%s1_attitude ([COLOR_WARNING_TEXT]%d2[COLOR_REVERT]) gegen&#252;ber %s3_leader</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_POSITIVE_NEUTRAL_ATTITUDE_TOWARDS</Tag>
		<English>%s1_attitude (+%d2) towards %s3_leader</English>
		<German>%s1_attitude (+%d2) gegen&#252;ber %s3_leader</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_NEGATIVE_NEUTRAL_ATTITUDE_TOWARDS</Tag>
		<English>%s1_attitude (%d2) towards %s3_leader</English>
		<German>%s1_attitude (%d2) gegen&#252;ber %s3_leader</German>
	</TEXT>
	<TEXT>
		<Tag>TXT_KEY_ZERO_ATTITUDE_TOWARDS</Tag>
		<English>%s1_attitude (+/- 0) towards %s2_leader</English>
		<German>%s1_attitude (+/- 0) gegen&#252;ber %s2_leader</German>
	</TEXT>
	<!-- advc.sha: Works differently in AdvCiv, and "we feel threatened by
		 your large civ" no longer fits well. -->
